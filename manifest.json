{
  "manifest_version": 3,
  "name": "Réorganisation des Onglets",
  "description": "Réorganiser, grouper et classer les onglets par catégories ou couleurs",
  "version": "1.0",
  "permissions": [
  "tabs",
  "storage",
  "tabGroups",
  "scripting"
  ],
  "background": {
  "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"    
  },
  "default-icon" : {
            "16" : "./a16.png",
            "48" : "./a48.png",
            "128" : "./a128.png"
  }
}
