{
  "manifest_version": 3,
  "name": "Réorganisation des Onglets",
  "description": "Réorganiser, grouper et classer les onglets par catégories ou couleurs",
  "version": "1.0",
  "permissions": [
  "tabs",
  "storage",
  "tabGroups",
  "scripting"
  ],
  "background": {
  "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"    
  },
  "icons": {
    "32": "./icons.png"
}
}
